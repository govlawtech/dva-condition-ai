    "status": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/status"
      },
      "backendUri": "https://dvasopapi.azurewebsites.net/status"
    },
    "suggestCondition": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/suggestSoPCondition"
